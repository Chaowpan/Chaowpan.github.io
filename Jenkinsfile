pipeline {
    agent any
    stages{
        stage('Clone') {
            steps {
                print "clone docker"
            }
        }
        stage('Build') {
            steps {
                print "Hello Jenkins"
        }
        }
        stage('Deploy Image') {
            steps {
                print "deploy image"
            }
        }
        stage('Testing') {
            steps {
                print "testing"
            }
        }
    }
}